<h2>Liste des collections</h2>

<table class="[ oocss-table ]">
   <thead>
        <th>nom</th>
        <th>type d'objet</th>
        <th>description</th>
   </thead>

   {% for collection in collections %}
        <tr class="[ oocss-table-row ]">
            <td class="oocss-table-data">{{ collection.name }}</td>
            <td class="oocss-table-data">{{ collection.entity }}</td>
            <td class="oocss-table-data">{{ collection.description }}</td>
        </tr>
   {% endfor %}
</table>
